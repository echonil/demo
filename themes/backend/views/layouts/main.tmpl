{{ import "partials/head.tmpl" }}
{{ import "partials/header.tmpl" }}
{{ import "partials/alert.tmpl" }}
{{ import "partials/footer.tmpl" }}
<!DOCTYPE html>
<html>
<head>
  {{ yield head() }}
</head>
<body class="hold-transition sidebar-mini layout-fixed">
  <div class="wrapper">
    {{ yield header() }}

    <div class="content-wrapper">
      <section class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1>{{ yield title() }}</h1>
            </div>
            <div class="col-sm-6">
              <ol class="breadcrumb float-sm-right">
                <li class="breadcrumb-item"><a href="{{ route("/console/home") }}">Home</a></li>
                <li class="breadcrumb-item active">{{ yield title() }}</li>
              </ol>
            </div>
          </div>
        </div>
      </section>

      <section class="content">
        {{ yield alert() }}

        {{ yield body() }}
      </section>
    </div>

    {{ yield footer() }}
  </div>

  <script src="https://cdn.jsdelivr.net/npm/jquery@3.4.1/dist/jquery.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.bundle.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/overlayscrollbars@1.11.0/js/OverlayScrollbars.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/admin-lte@3.0.2/dist/js/adminlte.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/admin-lte@3.0.2/dist/js/pages/dashboard.js"></script>
</body>
</html>
